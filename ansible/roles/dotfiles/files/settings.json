{
    "workbench.startupEditor": "newUntitledFile",
    "editor.fontFamily": "Operator Mono",
    "editor.wordWrap": "on",
    "editor.scrollBeyondLastLine": false,
    "window.title": "${activeEditorMedium}${separator}${rootName}${dirty}",
    "terminal.integrated.fontFamily": "Source Code Pro",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.scrollback": 1000,
	"javascript.validate.enable": false,
    "css.validate": false,
    "scss.validate": false,
    "less.validate": false,
    "gitlens.statusBar.alignment": "left",
    "gitlens.statusBar.format": "${id} - ${author}, ${ago} - ${message|40}",
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "terminal.integrated.confirmOnExit": true,
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Gruvbox Dark (Soft)",
    "bracketPairColorizer.timeOut": 0,
    "gitlens.blame.format": "${author|7} - ${message|20?} ${ago|14-}",
    "gitlens.hovers.currentLine.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.keymap": "chorded",
    "auto-close-tag.activationOnLanguage": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "plaintext",
        "markdown",
        "vue"
    ],
    "gitlens.advanced.messages": {
        "suppressGitVersionWarning": true,
        "suppressShowKeyBindingsNotice": true
    },
    "workbench.editor.labelFormat": "short",
    "workbench.editor.enablePreview": false,
    "extensions.ignoreRecommendations": false,
    "editor.quickSuggestions": {
        "other": false,
        "comments": true,
        "strings": false
    },
    "editor.quickSuggestionsDelay": 10,
    "editor.wordBasedSuggestions": false,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "python.linting.pylintPath": "/usr/local/bin/pylint",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "phpcs.executablePath": "/home/vagrant/beeswaxio/beeswax/buzz_api/buzz/vendor/bin/phpcs",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.iconTheme": "material-icon-theme",
    "search.exclude": {
        "**/vendor": true,
        "**/node_modules": true,
        "**/public": true,
        "**/.git": true,
        ".vscode": true
    },
    "html.format.endWithNewline": true,
    "html.format.indentInnerHtml": true,
    "html.format.wrapLineLength": 0,
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "git.ignoreLegacyWarning": true,
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#135564",
        "editor.selectionHighlightBackground": "#135564"
    },
    "typescript.updateImportsOnFileMove.enabled": "prompt",
    "files.watcherExclude": {
        "**/.git/**": true,
        "**/*.pyc": true,
        "**/node_modules/**": true,
        "**/bazel-beeswaxio": true,
        "**/bazel-bin": true,
        "**/bazel-out": true,
        "**/bazel-testlogs": true,
        "**/bazel-genfiles/**": true,
    },
    "files.exclude": {
        "**/.git/**": true,
        "**/*.pyc": true,
    },
    "phpcs.ignorePatterns": [
        "**/vendor/**"
    ],
    "phpcs.composerJsonPath": "./beeswax/buzz_api/buzz/composer.json",
    "eslint.alwaysShowStatus": true,
    "intelephense.environment.documentRoot": "./beeswax/buzz_api/buzz/api/api.php",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": false,
}
